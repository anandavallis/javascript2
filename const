<body>
    <script>
        const me ={
              name : 'David',
              age : 30,
              occuppation : 'web developer'
            };
    
             me = {
              name : 'Rob',
              age : 20,
              occuppation : null
            };
            console.log(me);
    </script>
</body>
</html>
