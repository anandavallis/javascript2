    <script>
        let length = 10;
        let width = 5;
        let area = rectangleArea(length, width);     

    function rectangleArea(length, width) {
    let area = length * width;
    return area;
}
console.log("The area of the rectangle with length " + length + " and width " + width + " is " + area);



    </script>
