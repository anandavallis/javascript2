<body>
    <script>
    function calculateResult(a, b, c, d) {
    let result = ((a + b) * c) / d;
    return result;
  }
   let a = 10;
   let b = 20;
   let c = 15;
   let d = 5;
   let result = calculateResult(a, b, c, d);
   console.log("( " + a + " + " + b + " ) * " + c + " / " + d + " = " + result);

    </script>
</body>
