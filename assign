    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Assignment Operator</title>
</head>
<body>
    <script>
        let num3 = 10;
      //Assignment operator  
          num3 = 20;
          console.log(num3);

        let num = 10;
        //Addition Assignment Operator
        num += 20;
        console.log(num);

        let x =20;
        //Subtraction Asignment Operator
        x -= 30;
        console.log(x);

        let num1 =30;
        //Multiplication Assignment Operator
        num1 *= 10;
        console.log(num1);

        let num2 =10;
        //Division Asssignment Operator
        num2 /= 20;
        console.log(num2);
    </script>
</body>
</html>
