    <title>Average</title>
</head>
<body>
    <script>

function findAverage(numbers) {
    let sum = 0;
    for (let i = 0; i < numbers.length; i++) {
        sum += numbers[i];
    }
    let average = sum / numbers.length;
    return average;
}

let numbers = [10, 20, 30, 40, 50];
let average = findAverage(numbers);

console.log("Numbers:", numbers);
console.log("Average:", average);
    </script>
</body>
</html>
